<div class="container">
  <h1>アップロード</h1>
  <p><%= flash.notice %></p>
  <%= form_for [:admin, @past_question] do |f| %>

  <%= render 'form', f: f %>

  <tr>
    <th><%= f.label :ファイルアップロード %></th>
    <td><%= f.file_field :images, multiple: true %></td>
  </tr>
  <p>
  	<%= f.submit %>
  </p>

  <% end %>
</div>
